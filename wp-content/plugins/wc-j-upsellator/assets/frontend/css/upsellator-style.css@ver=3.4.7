@charset "UTF-8";
@font-face {
    font-family: 'fontello';
    src: url('../../font/fontello.eot@80747879');
    src: url('../../font/fontello.eot@80747879') format('embedded-opentype'),
         url('../../font/fontello.woff2@80747879') format('woff2'),
         url('../../font/fontello.woff@80747879') format('woff'),
         url('../../font/fontello.ttf@80747879') format('truetype'),
         url('../../font/fontello.svg@80747879') format('svg');
    font-weight: normal;
    font-style: normal;
  }

[class^="wooj-icon-"]:before, [class*=" wooj-icon-"]:before 
{
    font-family: "fontello";
    font-style: normal;
    font-weight: normal;
    speak: never;    
    display: inline-block;
    text-decoration: inherit;
    width: 1em;
    margin-right: .2em;
    text-align: center;
    font-variant: normal;
    text-transform: none;
    line-height: 1em;
    margin-left: .2em;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
} 

